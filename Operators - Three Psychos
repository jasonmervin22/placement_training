import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int x1 = sc.nextInt();
        int y1 = sc.nextInt();
        int x2 = sc.nextInt();
        int y2 = sc.nextInt();
        
        float midx = (x1 + x2)/ 2.0f;
        float midy = (y1 + y2)/ 2.0f;
    
        System.out.printf("Arun's house is located at(%.1f,%.1f)",midx,midy);
    }
}
